{
  "name": "turborepo-remote-cache-remix",
  "description": "Self hosted Turborepo remote cache",
  "keywords": [
    "turborepo",
    "remote-cache",
    "monorepo",
    "remix"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-s lint build:*",
    "build:type": "tsc",
    "build:css": "tailwindcss -o ./app/tailwind.css --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "migrate": "prisma migrate",
    "lint": "eslint ./app --color --max-warnings=0",
    "prestart": "run-s migrate:prod seed",
    "migrate:prod": "prisma migrate deploy",
    "seed": "prisma db seed",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "abstract-blob-store": "^3.3.5",
    "aws-sdk": "^2.1140.0",
    "azure-blob-storage": "^4.2.5",
    "azure-blob-store": "^1.0.7",
    "fs-blob-store": "^6.0.0",
    "prisma": "^3.14.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-gravatar": "^2.6.3",
    "react-table": "^7.8.0",
    "remix": "^1.5.1",
    "remix-auth": "^3.2.2",
    "remix-auth-form": "^1.1.2",
    "remix-utils": "^3.2.0",
    "s3-blob-store": "^4.1.1",
    "tiny-invariant": "^1.2.0",
    "ts-node": "^10.8.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@tailwindcss/typography": "^0.5.2",
    "@thimar/eslint-config": "^1.2.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-gravatar": "^2.6.10",
    "@types/react-table": "^7.7.12",
    "@types/readable-stream": "^2.3.13",
    "@types/uuid": "^8.3.4",
    "daisyui": "^2.15.0",
    "eslint": "^8.16.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts"
  }
}
