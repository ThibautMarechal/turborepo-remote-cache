{
  "name": "@thimar/turborepo-remote-cache",
  "version": "1.9.0",
  "description": "Self hosted Turborepo remote cache",
  "sideEffects": false,
  "keywords": [
    "turborepo",
    "remote-cache",
    "monorepo",
    "remix"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "docker": "docker-compose -f docker-compose.db.yml up -d",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint ./app --color --max-warnings=0",
    "setup": "prisma migrate dev && prisma db seed",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8080 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress && tsc -b prisma",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "prestart": "prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production remix-serve build"
  },
  "dependencies": {
    "@heroicons/react": "2.0.14",
    "@prisma/client": "^4.6.0",
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/serve": "1.12.0",
    "@tanstack/react-table": "8.7.9",
    "abstract-blob-store": "3.3.5",
    "aws-sdk": "2.1309.0",
    "azure-blob-storage": "4.2.5",
    "azure-blob-store": "1.0.7",
    "classnames": "2.3.2",
    "copy-to-clipboard": "3.3.3",
    "cross-env": "7.0.3",
    "debug": "^4.3.4",
    "domain-functions": "^1.3.0",
    "fs-blob-store": "6.0.0",
    "isbot": "^3.6.3",
    "npm-run-all": "4.1.5",
    "prisma": "^4.6.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-charts": "3.0.0-beta.54",
    "react-dom": "18.2.0",
    "react-gravatar": "2.6.3",
    "react-hook-form": "7.43.0",
    "react-intersection-observer": "9.4.1",
    "react-router-dom": "6.8.0",
    "remix-auth": "^3.3.0",
    "remix-auth-form": "^1.2.0",
    "remix-auth-oidc": "^1.0.0",
    "remix-forms": "^1.0.0",
    "remix-utils": "^6.0.0",
    "s3-blob-store": "4.1.1",
    "superjson": "^1.11.0",
    "tiny-invariant": "^1.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@remix-run/dev": "1.12.0",
    "@remix-run/eslint-config": "1.12.0",
    "@tailwindcss/typography": "0.5.9",
    "@testing-library/cypress": "9.0.0",
    "@testing-library/dom": "8.20.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@thimar/eslint-config": "2.0.1",
    "@types/node": "18.11.18",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-gravatar": "2.6.10",
    "@types/readable-stream": "2.3.15",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.28.0",
    "c8": "7.12.0",
    "cypress": "12.5.1",
    "daisyui": "2.50.0",
    "eslint": "8.33.0",
    "happy-dom": "8.2.6",
    "start-server-and-test": "1.15.3",
    "tailwindcss": "3.2.4",
    "vite": "^4.0.1",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.28.0"
  },
  "resolutions": {
    "react-charts/@types/react": "^18.0.9",
    "react-charts/@types/react-dom": "^18.0.4"
  },
  "prisma": {
    "seed": "ts-node  --transpile-only --require tsconfig-paths/register prisma/seed.ts"
  }
}
