{
  "name": "@thimar/turborepo-remote-cache",
  "version": "1.4.0",
  "description": "Self hosted Turborepo remote cache",
  "sideEffects": false,
  "keywords": [
    "turborepo",
    "remote-cache",
    "monorepo",
    "remix"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "docker": "docker-compose -f docker-compose.db.yml up -d",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint ./app --color --max-warnings=0",
    "setup": "prisma migrate dev && prisma db seed",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8080 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "prestart": "prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production remix-serve build"
  },
  "dependencies": {
    "@heroicons/react": "2.0.10",
    "@prisma/client": "4.3.1",
    "@remix-run/express": "1.7.0",
    "@remix-run/node": "1.7.0",
    "@remix-run/react": "1.7.0",
    "@remix-run/serve": "1.7.0",
    "@tanstack/react-table": "8.5.13",
    "@types/node": "18.7.11",
    "@types/uuid": "8.3.4",
    "abstract-blob-store": "3.3.5",
    "aws-sdk": "2.1212.0",
    "azure-blob-storage": "4.2.5",
    "azure-blob-store": "1.0.7",
    "classnames": "2.3.1",
    "copy-to-clipboard": "3.3.2",
    "cross-env": "7.0.3",
    "fs-blob-store": "6.0.0",
    "npm-run-all": "4.1.5",
    "polly-js": "1.8.3",
    "prisma": "4.3.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-charts": "3.0.0-beta.48",
    "react-dom": "18.2.0",
    "react-gravatar": "2.6.3",
    "react-hook-form": "7.34.2",
    "react-intersection-observer": "9.4.0",
    "react-router-dom": "6.3.0",
    "react-table": "7.8.0",
    "remix-auth": "3.2.2",
    "remix-auth-form": "1.2.0",
    "remix-domains": "0.3.2",
    "remix-forms": "0.17.4-test.0",
    "remix-utils": "3.3.0",
    "s3-blob-store": "4.1.1",
    "superjson": "1.9.1",
    "tiny-invariant": "1.2.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.3",
    "uuid": "9.0.0",
    "zod": "3.19.0"
  },
  "devDependencies": {
    "@faker-js/faker": "7.5.0",
    "@remix-run/dev": "1.7.0",
    "@remix-run/eslint-config": "1.7.0",
    "@tailwindcss/typography": "0.5.7",
    "@testing-library/cypress": "8.0.3",
    "@testing-library/dom": "8.17.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@thimar/eslint-config": "2.0.1",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-gravatar": "2.6.10",
    "@types/react-table": "7.7.12",
    "@types/readable-stream": "2.3.14",
    "@vitejs/plugin-react": "2.1.0",
    "@vitest/coverage-c8": "0.23.1",
    "c8": "7.12.0",
    "cypress": "10.7.0",
    "daisyui": "2.25.0",
    "eslint": "8.23.0",
    "happy-dom": "6.0.4",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "3.1.8",
    "vite": "3.0.9",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.23.1"
  },
  "resolutions": {
    "react-charts/@types/react": "^18.0.9",
    "react-charts/@types/react-dom": "^18.0.4"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
